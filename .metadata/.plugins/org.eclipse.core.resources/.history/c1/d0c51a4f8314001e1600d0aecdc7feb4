package com.onlinemarket.service;

import org.springframework.beans.factory.annotation.Autowired;

import com.onlinemarket.entity.Customers;

public class CreateNewCustomerService implements CreateNewCustomerServiceInterface {
	
	@Autowired

	CreateNewCustomerDao  createCustomerDao;

	public Customers createNewCustomer(String customerId, String firstName, String lastName, int phoneNumber) {
		
		
		return createCustomerDao.reateNewCustomer( customerId, firstName, lastName, phoneNumber);
	}
	

}
