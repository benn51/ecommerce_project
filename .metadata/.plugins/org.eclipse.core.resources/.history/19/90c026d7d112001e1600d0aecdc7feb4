		String sql= " "
				+ "DELETE FROM shoes "
				+ " WHERE shoe_id = :shoe:id ";
				
		
		
		
		return null;
	}
	
	
	class SqlParams{
		String sql;
		MapSqlParameterSource source = new MapSqlParameterSource();
	      

	    }

