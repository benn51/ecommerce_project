import lombok.Data;
import lombok.NoArgsConstructor;
@NoArgsConstructor
@AllArgsConstructor
@Data
@Builder


public class Cloths {
 
	private int clothPk;
	private String clothId;
	private int  clothSize;
	private String clothColor;
	
	private CatagoryType catagory = CatagoryType.PERSONAL_WEAR;
	private int price;
	
		
}








